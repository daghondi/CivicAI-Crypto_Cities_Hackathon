# Smart Contract Deployment Configuration

# Private key for deployment (DO NOT commit this to git)
PRIVATE_KEY=your_private_key_here

# Network URLs
SEPOLIA_URL=https://sepolia.infura.io/v3/your-infura-project-id
OPTIMISM_SEPOLIA_URL=https://sepolia.optimism.io
MAINNET_URL=https://mainnet.infura.io/v3/your-infura-project-id

# Etherscan API keys for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key
OPTIMISM_ETHERSCAN_API_KEY=your_optimistic_etherscan_api_key

# Gas reporting
REPORT_GAS=false

# Optional: Infura or Alchemy project IDs
INFURA_PROJECT_ID=your_infura_project_id
ALCHEMY_API_KEY=your_alchemy_api_key
